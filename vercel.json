{
    "framework": "nextjs",
    "buildCommand": "cd web-frontend && npm run build",
    "outputDirectory": "web-frontend/.next",
    "installCommand": "cd web-frontend && npm install --legacy-peer-deps",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "http://localhost:8000/api/:path*"
        }
    ]
}